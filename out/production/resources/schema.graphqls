type Person {
  id: ID
  name: String!
  age: Int!
  children: [Child!]!
}

type Child {
  name: String!
  age: Int!
  parentId: ID!
  favoriteCartoonId: Int!
}

type Query {
  allPersons(last: Int): [Person!]!
  personById(id: ID): Person
}

type Mutation {
  createPerson(name: String!, age: Int!): Person!
}